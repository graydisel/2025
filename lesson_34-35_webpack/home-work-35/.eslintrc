{
  "ecmaFeatures": {
    "modules": true,
    "spread" : true,
    "restParams" : true,
    "jsx": true
  },
  "rules": {
    "no-unused-vars": "warn"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": ["react", "react-hooks"]
}